

.session-footer {
    padding-top: 10pt;
    border-top: 2pt solid color-gray-6;
    display: flex;
    width: 100%;
    height: 100%;
}

.footer__links {
    width: 100%;
}

.footer__link {
    display: flex;

    &-name {
        width: 60pt;
        font-weight: 400;
        color: color-gray-4;
        font-size: 9pt;
    }

    &-a {
        font-size: 9pt;
    }
}

.footer__logo {
    height: 100%;
    display: flex;
    align-items: center;
}

.footer__image {
    width: 1.38cm;
    height: 1.38cm;
}