.list {
    padding: 0;
    margin: 0;
    list-style: none;
    width: 100%;
    display: flex;
    align-items: center;
    flex-wrap: wrap;

    li {
        display: flex;
        align-items: center;
        line-height: 1;
    }
}

.item {
    color: color-gray-3;
    font-weight: 400;
    font-size: 9pt;
}

.isPrimary {
    color: color-azure-4;
}

.separator {
    font-weight: 400;
    color: color-gray-5;
    margin-inline: 2pt;
}